		<xs:element name="PersonalInfo" id="PersSec1">
			<xs:annotation>
				<xs:documentation>Enter personal information</xs:documentation>
			</xs:annotation>
			<xs:complexType>
				<xs:sequence>
					<xs:element ref="AssesseeName" id="PersSec2">
						<xs:annotation>
							<xs:documentation>Assessee name with Sur Name or Org Name
								mandatory.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PAN" id="PersSec3">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:length value="10" />
								<xs:pattern value="[A-Z][A-Z][A-Z][A-Z][A-Z]\d\d\d\d[A-Z]" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element ref="Address" id="PersSec4">
						<xs:annotation>
							<xs:documentation> Address of assessee</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DOB" id="PersSec5">
						<xs:simpleType>
							<xs:restriction base="xs:date">
	
								<xs:maxInclusive value="2013-03-31" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="EmployerCategory" default="OTH" id="PersSec6">
						<xs:annotation>
							<xs:documentation>For HUF select NA for Employer category
							</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:token">
								<xs:maxLength value="3" />
								<xs:minLength value="2" />
								<xs:enumeration value="GOV" />
								<xs:enumeration value="PSU" />
								<xs:enumeration value="OTH" />
								<xs:enumeration value="NA" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="Gender" default="M" id="PersSec7">
						<xs:annotation>
							<xs:documentation>For HUF select X for Gender</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:token">
								<xs:maxLength value="1" />
								<xs:enumeration value="M" />
								<xs:enumeration value="F" />
								<xs:enumeration value="X" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="Status" default="I" id="PersSec8">
						<xs:simpleType>
							<xs:restriction base="xs:token">
								<xs:maxLength value="1" />
								<xs:enumeration value="I" />
								<xs:enumeration value="H" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
		</xs:element>
		<xs:element name="AssesseeName" id="AseNamSec0">
			<xs:annotation>
				<xs:documentation>Assessee name with Surname mandatory.
				</xs:documentation>
			</xs:annotation>
			<xs:complexType>
				<xs:sequence>
					<xs:element name="FirstName" id="AseNamSec1" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="25" />
	
								<xs:minLength value="0" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="MiddleName" id="AseNamSec2" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="25" />
	
								<xs:minLength value="0" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="SurNameOrOrgName" id="AseNamSec3">
						<xs:annotation>
							<xs:documentation>Enter Last or Sur name for Individual or HUF
								name here
							</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="75" />
								<xs:minLength value="1" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="OrgOldName" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="75" />
								<xs:minLength value="0" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
		</xs:element>
		<xs:element name="Address" id="AdrSec0">
			<xs:annotation>
				<xs:documentation> Address of assessee</xs:documentation>
			</xs:annotation>
			<xs:complexType>
				<xs:sequence>
					<xs:element name="ResidenceNo" id="AdrSec1">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="50" />
								<xs:minLength value="1" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="ResidenceName" id="AdrSec2" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:minLength value="0" />
								<xs:maxLength value="50" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="RoadOrStreet" id="AdrSec3" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="50" />
								<xs:minLength value="0" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="LocalityOrArea" id="AdrSec4">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="50" />
								<xs:minLength value="1" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="CityOrTownOrDistrict" id="AdrSec5">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:minLength value="1" />
								<xs:maxLength value="50" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="StateCode" id="AdrSec6">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="0[1-9]{1}|1[0-9]{1}|2[0-9]{1}|3[0-5]{1}|99" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element ref="CountryCode" id="AdrSec13" />
					<xs:element name="PinCode" id="AdrSec7">
						<xs:annotation>
							<xs:documentation>Fill 999999 in case of Foreign Address
							</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:unsignedLong">
								<xs:totalDigits value="6" />
								<xs:pattern value="\d\d\d\d\d\d" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="Phone" id="AdrSec8" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="STDcode" id="AdrSec9" default="0">
									<xs:simpleType>
										<xs:restriction base="xs:unsignedLong">
											<xs:totalDigits value="5" />
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="PhoneNo" id="AdrSec10" default="0">
									<xs:simpleType>
										<xs:restriction base="xs:unsignedLong">
											<xs:totalDigits value="10" />
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="MobileNo" id="AdrSec12" minOccurs="1">
						<xs:simpleType>
							<xs:restriction base="xs:unsignedLong">
								<xs:totalDigits value="10" />
								<xs:pattern value="[1-9]{1}[0-9]{9}" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="MobileNoSec" id="AdrSec14" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:unsignedLong">
								<xs:totalDigits value="10" />
								<xs:pattern value="[1-9]{1}[0-9]{9}" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="EmailAddress" id="AdrSec11">
						<xs:annotation>
							<xs:documentation>Email Id is required for receiving copy of ITR-V
							</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="125" />
								<xs:pattern
									value="([\.a-zA-Z0-9_\-])+@([a-zA-Z0-9_\-])+(([a-zA-Z0-9_\-])*\.([a-zA-Z0-9_\-])+)+" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="EmailAddressSec" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Alternate Email Id</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="125" />
								<xs:pattern
									value="([\.a-zA-Z0-9_\-])+@([a-zA-Z0-9_\-])+(([a-zA-Z0-9_\-])*\.([a-zA-Z0-9_\-])+)+" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
		</xs:element>
		<xs:element name="AssesseeRep" id="AseRepSec0">
			<xs:annotation>
				<xs:documentation> Assessee representative</xs:documentation>
			</xs:annotation>
			<xs:complexType>
				<xs:sequence>
					<xs:element name="RepName" id="AseRepSec2">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="125" />
								<xs:minLength value="1" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="RepAddress" id="AseRepSec3">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="200" />
								<xs:minLength value="1" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="RepPAN" id="AseRepSec4" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:length value="10" />
								<xs:pattern value="[A-Z][A-Z][A-Z][A-Z][A-Z]\d\d\d\d[A-Z]" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
		</xs:element>
		<xs:element name="AuditInfo" id="AudiSec0">
			<xs:annotation>
				<xs:documentation> Audit Information</xs:documentation>
			</xs:annotation>
			<xs:complexType>
				<xs:sequence>
					<xs:element name="LiableSec44AAflg" default="N" id="AudiSec1">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:length value="1" />
								<xs:pattern value="N|Y" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="LiableSec44ABflg" default="N" id="AudiSec2">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:length value="1" />
								<xs:pattern value="N|Y" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="AuditReportFurnishDate" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:date">
								<xs:minInclusive value="2013-04-01" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="AuditorName" id="AudiSec3" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="125" />
								<xs:minLength value="1" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="AuditorMemNo" id="AudiSec4" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:length value="6" />
								<xs:pattern value="\d\d\d\d\d\d" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="AudFrmName" id="AudiSec5" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="125" />
								<xs:minLength value="1" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="AudFrmPAN" id="AudiSec6" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Enter Firm's PAN or Proprietor's PAN here
							</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:length value="10" />
								<xs:pattern value="[A-Z][A-Z][A-Z][A-Z][A-Z]\d\d\d\d[A-Z]" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="AuditDate" id="AudiSec7" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:date">
								<xs:minInclusive value="2012-04-01" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="RepSec92EFlag">
						<xs:annotation>
							<xs:documentation>Are you liable to furnish a report under Section
								92E ?
							</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Y" />
								<xs:enumeration value="N" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
		</xs:element>
		<xs:element name="IncomeDeductions" id="IncDedSec0">
			<xs:annotation>
				<xs:documentation> Income and deduction details</xs:documentation>
			</xs:annotation>
			<xs:complexType>
				<xs:sequence>
					<xs:element name="IncomeFromSal" default="0" id="IncDedSec1"
						minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:unsignedLong">
								<xs:totalDigits value="14" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element ref="IncomeOthSrc" id="IncDedSec2" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Income chargeable under the Head ‘Other
								Sources’ (Interest)
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GrossTotIncome" type="xs:long" default="0"
						id="IncDedSec3" />
					<xs:element ref="DeductUndChapVIA" id="IncDedSec4">
						<xs:annotation>
							<xs:documentation>Deductions from income</xs:documentation>
